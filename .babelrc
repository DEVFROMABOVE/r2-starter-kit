{
  "env": {
    "test": {
      "presets": ["react", "es2015", "stage-0"],
      "plugins": [
        "babel-plugin-rewire"
      ]
    },
    "development": {
      "presets": ["react-hmre"],
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": "react-transform-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }]
      }]]
    }
  }
}
